#!/bin/bash

/root/ipv6/ipv6.bash

. /root/scripts/channels.bash

mkdir -p /tmp/live-dl

for key in "${!CHANNEL_IDS[@]}"; do
    # Look at live-dl-gen.example
    /usr/bin/nohup /bin/bash /root/scripts/live-dl-gen "https://www.youtube.com/channel/${CHANNEL_IDS[${key}]}" &> "/tmp/live-dl/${key}.log" &
done

/root/scripts/isBanned.bash &